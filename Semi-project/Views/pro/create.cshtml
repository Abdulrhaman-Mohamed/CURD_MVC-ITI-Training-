@model Employee
@{
    ViewBag.Title = "create";
    Layout = "~/Views/TheFirst.cshtml";
}

<form method="post" action="/pro/create">
    <div class="row mb-3">
        <label for="name_" class="col-sm-2 col-form-label">Name</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="name_" name="name_" ">
        </div>
    </div>
    <div class="row mb-3">
        <label for="ID" class="col-sm-2 col-form-label">ID</label>
        <div class="col-sm-10">
            <input type="number" class="form-control" id="ID" name="ID">
        </div>
    </div>
    <div class="row mb-3">
        <label for="age" class="col-sm-2 col-form-label">Age</label>
        <div class="col-sm-10">
            <input type="number" class="form-control" id="age" name="age">
        </div>
    </div>
    <div class="row mb-3">
        <label for="salary" class="col-sm-2 col-form-label">Salary</label>
        <div class="col-sm-10">
            <input type="number" class="form-control" id="salary" name="salary">
        </div>
    </div>
    <div class="row mb-3">
        <label for="Dept_ID" class="col-sm-2 col-form-label">Department</label>
        <div class="col-sm-10">
            <input type="number" class="form-control" id="Dept_ID" name="Dept_ID" />
        </div>
    </div>



    <input type="submit" class="btn btn-primary" />


</form>
<a href="/pro/Thepage" class="btn btn-outline-secondary"> Back To Home</a>

